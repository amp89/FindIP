@Controller
@SessionAttributes({"currentUserLogin"})
public class LoginController {
	
	@ModelAttribute("currentUserLogin")
	private CurrentUser setBlankCurrentUserLogin(){
		return new CurrentUser();
	}
	
	@Autowired
	private IpDAO dao;
	
